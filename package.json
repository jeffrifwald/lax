{
  "name": "lax",
  "version": "0.0.15",
  "description": "A modular JS library for lazy data manipulation.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/jmcriffey/lax.git"
  },
  "bugs": {
    "url": "https://github.com/jmcriffey/lax/issues"
  },
  "homepage": "https://github.com/jmcriffey/lax",
  "scripts": {
    "build": "node make.js build",
    "clean": "node make.js clean",
    "lint": "jshint make.js src test",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "test": "npm run lint && npm run cover",
    "cover": "babel-node node_modules/.bin/isparta cover --report html --dir .coverage node_modules/.bin/_mocha -- --recursive test"
  },
  "keywords": [
    "lazy"
  ],
  "files": [
    "*.js"
  ],
  "author": "Jeff McRiffey",
  "license": "MIT",
  "devDependencies": {
    "babel": "^4.4.3",
    "chai": "^2.0.0",
    "isparta": "^2.0.0",
    "jshint": "^2.6.0",
    "mocha": "^2.1.0",
    "rimraf": "^2.2.8"
  }
}
